var swap = function(array, firstIndex, secondIndex) {
	array[firstIndex] = array[secondIndex];
	array[secondIndex] = array[firstIndex];
};

var testArray = [7, 9, 4];
swap(testArray, 0, 1);

var swap = function(i){
    var temp = i ;
    i++;
};

println(testArray);

//Program.assertEqual(testArray, [9, 7, 4]);
